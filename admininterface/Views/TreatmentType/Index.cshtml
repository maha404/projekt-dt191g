@model IEnumerable<admininterface.Models.TreatmentTypeModel>

@{
    ViewData["Title"] = "Hantera behandlingstyp";
}

<h1>Hantera behandlingstyp</h1>

<p>
    <a asp-action="Create"> + Lägg till</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>Hantera</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Ändra</a> 
                <a asp-action="Details" asp-route-id="@item.Id">Detaljer</a> 
                <a asp-action="Delete" asp-route-id="@item.Id">Radera</a>
            </td>
        </tr>
}
    </tbody>
</table>
